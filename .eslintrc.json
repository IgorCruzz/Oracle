{
  "env": {
    "es2021": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "space-before-function-paren": ["error", { "named": "never" }],
    "no-console": "off"
  }
}
