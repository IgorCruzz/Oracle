{
  "name": "gerobras",
  "version": "1.0.3",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "deploy": "sucrase ./src -d ./deploy --transforms imports",
    "start": "pm2 start deploy/server.js --name GEROBRAS && pm2 log",
    "dev": "nodemon src/server.js",
    "build": "tsup-node src/data/database/config --format esm --outDir dist/data/database/config && tsup-node src/data/database/migrations --format cjs --outDir dist/data/database/migrations && tsup-node src/data/database/models --format cjs --outDir dist/data/database/models",
    "db-create": "yarn sequelize migration:create",
    "db-migrate": "yarn sequelize db:migrate"   
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql2": "^2.3.3",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sqlite3": "^5.0.2",
    "tsup-node": "^0.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^2.0.15",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sucrase": "^3.10.1"
  }
}
